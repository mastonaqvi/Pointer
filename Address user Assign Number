#include<iostream>
using namespace std;
int main()
{
	int s;
	int Arr[10];
	int *ptr = Arr;
	cout << "Enter Size of values : ";
	cin >> s;
	

	
	
	for (int i = 1; i < s+1; i++)
	{
		cout << "Enter Values: " << i << endl;
		cin >> *(ptr+i);
	}
	for (int i = 0; i < 10; i++)
	{
		cout << *(ptr + i) << endl;
		cout << (ptr + i) << endl;
	}
	system("pause");
	return 0;
}
